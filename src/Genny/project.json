{
  "description": "Genny, the Dotnet code scaffolder",
  "packOptions": {
    "projectUrl": "https://github.com/Muchiachio/Genny",
    "licenseUrl": "http://opensource.org/licenses/MIT",
    "tags": [ "dotnet scaffolding" ]
  },
  "authors": [ "Muchiachio" ],
  "version": "1.0.0",

  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Razor": "1.0.0",
    "Microsoft.DotNet.ProjectModel": "1.0.0-rc3-003121",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0",
    "Microsoft.DotNet.Cli.Utils": "1.0.0-preview2-003121"
  },

  "buildOptions": {
    "outputName": "dotnet-gen",
    "warningsAsErrors": true,
    "emitEntryPoint": true
  },

  "frameworks": {
    "net451": { },
    "netcoreapp1.0": {
      "dependencies": {
        "System.Runtime.Loader": "4.0.0",
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  }
}
